<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/imageView75"
        android:scaleType="fitXY"
        android:src="@drawable/result_back"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <RelativeLayout
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:onClick="doBack"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:background="@drawable/result_btnkt"
        android:layout_marginTop="16dp"
        android:id="@+id/relativeLayout12"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="90dp"
        android:text="0"
        android:textSize="40dp"
        android:id="@+id/textView60"
        android:gravity="center"
        android:layout_marginRight="30dp"
        android:textColor="@android:color/white"
        android:layout_alignTop="@+id/relativeLayout12"
        android:layout_toLeftOf="@+id/relativeLayout12"
        android:layout_toStartOf="@+id/relativeLayout12" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="90dp"
        android:text="0"
        android:gravity="center"
        android:textSize="40dp"
        android:textColor="@android:color/black"
        android:layout_marginLeft="30dp"
        android:id="@+id/textView61"
        android:layout_alignBottom="@+id/textView60"
        android:layout_toRightOf="@+id/relativeLayout12"
        android:layout_toEndOf="@+id/relativeLayout12" />

    <TextView
        android:layout_width="65dp"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:gravity="center"
        android:maxLines="1"
        android:textColor="@android:color/white"
        android:layout_margin="16dp"
        android:id="@+id/textView62"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <com.kt.ktball.myclass.MyCircleImageView
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:id="@+id/view"
        android:src="@drawable/default_head"
        android:layout_above="@+id/textView62"
        android:layout_alignLeft="@+id/textView62"
        android:layout_alignStart="@+id/textView62" />

    <TextView
        android:layout_width="65dp"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:layout_margin="16dp"
        android:textColor="@android:color/black"
        android:id="@+id/textView63"
        android:gravity="center"
        android:maxLines="1"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:layout_width="65dp"
        android:layout_height="30dp"
        android:id="@+id/imageView76"
        android:visibility="gone"
        android:src="@drawable/result_win"
        android:layout_alignBottom="@+id/view"
        android:layout_alignLeft="@+id/view"
        android:layout_alignStart="@+id/view" />

    <com.kt.ktball.myclass.MyCircleImageView
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:id="@+id/view2"
        android:src="@drawable/default_head"
        android:layout_alignBottom="@+id/view"
        android:layout_alignRight="@+id/textView63"
        android:layout_alignEnd="@+id/textView63" />

    <ImageView
        android:layout_width="65dp"
        android:layout_height="30dp"
        android:id="@+id/imageView77"
        android:visibility="gone"
        android:src="@drawable/result_win"
        android:layout_above="@+id/textView63"
        android:layout_alignLeft="@+id/view2"
        android:layout_alignStart="@+id/view2" />

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/imageView78"
        android:visibility="gone"
        android:src="@drawable/result_kt"
        android:layout_alignBottom="@+id/view"
        android:layout_toRightOf="@+id/view"
        android:layout_toEndOf="@+id/view"
        android:layout_marginBottom="13dp" />

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/imageView79"
        android:visibility="gone"
        android:src="@drawable/result_kt"
        android:layout_marginBottom="21dp"
        android:layout_alignBottom="@+id/view2"
        android:layout_toLeftOf="@+id/view2"
        android:layout_toStartOf="@+id/view2" />

    <LinearLayout
        android:layout_width="130dp"
        android:layout_height="80dp"
        android:layout_alignBottom="@+id/textView62"
        android:layout_centerHorizontal="true"
        android:orientation="vertical">

        <TextView
            android:layout_width="130dp"
            android:layout_height="0dp"
            android:text="New Text"
            android:maxLines="1"
            android:textColor="@android:color/black"
            android:gravity="center"
            android:id="@+id/textView65"
            android:layout_weight="1" />

        <TextView
            android:layout_width="130dp"
            android:layout_height="0dp"
            android:text="New Text"
            android:maxLines="1"
            android:textColor="@android:color/black"
            android:gravity="center"
            android:id="@+id/textView64"
            android:layout_weight="1" />

        <TextView
            android:layout_width="130dp"
            android:layout_height="0dp"
            android:text="New Text"
            android:maxLines="1"
            android:textColor="@android:color/black"
            android:gravity="center"
            android:id="@+id/textView66"
            android:layout_weight="1" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/view"
        android:layout_marginLeft="15dp"
        android:layout_alignParentBottom="true"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/white"
                android:text="@string/winning_probability"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/white"
                android:id="@+id/activity_game_result_left_1"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/white"
                android:id="@+id/activity_game_result_left_2"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/white"
                android:id="@+id/activity_game_result_left_3"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/activity_game_result_left_user_a_ll"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/white"
                android:id="@+id/activity_game_result_left_user_a"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/activity_game_result_left_user_a_num"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/activity_game_result_left_user_b_ll"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/white"
                android:id="@+id/activity_game_result_left_user_b"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/activity_game_result_left_user_b_num"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/activity_game_result_left_user_c_ll"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/white"
                android:id="@+id/activity_game_result_left_user_c"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/activity_game_result_left_user_c_num"/>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="96dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:text="@string/winning_probability"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:id="@+id/activity_game_result_left_4"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:id="@+id/activity_game_result_left_5"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:id="@+id/activity_game_result_left_6"/>


        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/activity_game_result_left_user_d_ll"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:id="@+id/activity_game_result_left_user_d"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/activity_game_result_left_user_d_num"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/activity_game_result_left_user_e_ll"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:id="@+id/activity_game_result_left_user_e"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/activity_game_result_left_user_e_num"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/activity_game_result_left_user_f_ll"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/black"
                android:id="@+id/activity_game_result_left_user_f"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/activity_game_result_left_user_f_num"/>
        </LinearLayout>
    </LinearLayout>

</RelativeLayout>
