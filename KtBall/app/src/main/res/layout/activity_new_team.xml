<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UI.Activity.NewTeamActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@color/footTextColor"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/relativeLayout6">

        <RelativeLayout
        android:layout_width="60dp"
        android:layout_height="match_parent"
        android:onClick="doBack"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <ImageView
            android:id="@+id/imageView30"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_centerInParent="true"
            android:src="@drawable/retu" />
    </RelativeLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="战队名字"
            android:textColor="@android:color/white"
            android:textSize="20dp"
            android:id="@+id/textView36"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="完成"
            android:onClick="doNewTeamFinsh"
            android:textColor="@android:color/white"
            android:textSize="20dp"
            android:layout_marginRight="16dp"
            android:id="@+id/textView100"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:paddingRight="16dp"
        android:paddingLeft="16dp"
        android:layout_below="@+id/relativeLayout6"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/relativeLayout38">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="头像"
            android:textSize="17dp"
            android:textColor="@android:color/black"
            android:id="@+id/textView101"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <ImageView
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:id="@+id/imageView72"
            android:onClick="doGetPhoto"
            android:src="@drawable/iconfont_add"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:layout_below="@+id/relativeLayout38"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/relativeLayout39">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="战队名称"
            android:textSize="17dp"
            android:textColor="@android:color/black"
            android:id="@+id/textView102"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

         <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/editText10"
            android:hint="请填写战队名称"
            android:textSize="17dp"
            android:background="@drawable/no_border"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:paddingRight="16dp"
        android:paddingLeft="16dp"
        android:layout_below="@+id/relativeLayout39"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="战队类型"
            android:textSize="17dp"
            android:textColor="@android:color/black"
            android:id="@+id/textView103"
            android:layout_centerVertical="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <ImageView
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:id="@+id/imageView73"
            android:src="@drawable/new_team_buttom"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="--请选择战队类型--"
            android:id="@+id/textView104"
            android:onClick="doSeleteType"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@+id/imageView73"
            android:layout_toStartOf="@+id/imageView73" />
    </RelativeLayout>

    <RelativeLayout
        android:clickable="true"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#70000000"
        android:visibility="gone"
        android:id="@+id/relativeLayout888"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="180dp"
            android:background="@android:color/white"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true">


            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:text="拍照"
                android:onClick="doTakePhoto"
                android:textSize="20dp"
                android:textColor="@android:color/holo_blue_light"
                android:gravity="center"
                android:id="@+id/textView777"
                android:layout_weight="1" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/black"
                android:id="@+id/textView95" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:text="从相册中选择"
                android:textSize="20dp"
                android:onClick="doSeletePhoto"
                android:textColor="@android:color/holo_blue_light"
                android:gravity="center"
                android:id="@+id/textView788"
                android:layout_weight="1" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/black"
                android:id="@+id/textView1044" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:text="取消"
                android:onClick="doCanal"
                android:textSize="20dp"
                android:textColor="@android:color/holo_blue_light"
                android:gravity="center"
                android:id="@+id/textView799"
                android:layout_weight="1" />
        </LinearLayout>
    </RelativeLayout>

</RelativeLayout>
