<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#262626"
    tools:context=".UI.Activity.PasswordResetActivity">

    <RelativeLayout
        android:id="@+id/relativeLayout8"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginBottom="10dp">

        <ImageView
            android:id="@+id/imageView20"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:onClick="doFinsh"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:src="@drawable/retu" />

        <TextView
            android:id="@+id/textView29"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="@string/resert_pwd"
            android:textColor="@android:color/white"
            android:textSize="18dp" />
    </RelativeLayout>

    <ImageView
        android:id="@+id/imageView47"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/relativeLayout8"
        android:layout_centerHorizontal="true"
        android:src="@drawable/ktlogo"
        android:visibility="gone" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/imageView47"
        android:layout_marginTop="40dp"
        android:paddingLeft="10dp"
        android:paddingRight="10dp">

        <LinearLayout
            android:id="@+id/relativeLayout23"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="5dp"
            android:layout_marginTop="5dp"
            android:layout_alignParentTop="true">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="5dp"
                android:text="@string/check_phone"
                android:textColor="@color/colorHui"
                android:textSize="13dp" />

            <EditText
                android:id="@+id/editText3"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/no_border"
                android:ellipsize="end"
                android:ems="10"
                android:inputType="phone"
                android:maxLines="1"
                android:textCursorDrawable="@drawable/color_cursor_white"
                android:paddingLeft="10dp"
                android:textColor="@android:color/white"
                android:textSize="17dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="5dp"
                android:text="+86"
                android:textColor="@color/white" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_below="@+id/relativeLayout23"
            android:background="@color/colorHui">

        </View>

        <LinearLayout
            android:id="@+id/relativeLayout24"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/relativeLayout23"
            android:layout_marginBottom="5dp"
            android:layout_marginTop="5dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="5dp"
                android:text="@string/send_yanzhen"
                android:textColor="@color/colorHui"
                android:textSize="13dp" />

            <EditText
                android:id="@+id/editText5"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_alignParentBottom="true"
                android:layout_weight="1"
                android:background="@drawable/no_border"
                android:ems="10"
                android:textCursorDrawable="@drawable/color_cursor_white"
                android:inputType="phone"
                android:paddingLeft="10dp"
                android:textColor="@android:color/white"
                android:textSize="17dp" />

            <Button
                android:id="@+id/button2"
                android:layout_width="80dp"
                android:layout_height="25dp"
                android:layout_gravity="center"
                android:layout_marginRight="5dp"
                android:background="@mipmap/logo_dengl"
                android:onClick="doGetCode"
                android:text="@string/yanzmyanz"
                android:textColor="@android:color/black" />


        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_below="@+id/relativeLayout24"
            android:background="@color/colorHui">

        </View>

        <LinearLayout
            android:id="@+id/relativeLayout25"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_alignParentLeft="true"
            android:layout_marginBottom="5dp"
            android:layout_marginTop="5dp"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/relativeLayout24">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="5dp"
                android:text="@string/shurumima"
                android:textColor="@color/colorHui"
                android:textSize="13dp" />

            <EditText
                android:id="@+id/editText6"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/no_border"
                android:ems="10"
                android:inputType="textPassword"
                android:paddingLeft="10dp"
                android:textCursorDrawable="@drawable/color_cursor_white"
                android:textColor="@color/white"
                android:textSize="17dp" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_below="@+id/relativeLayout25"
            android:background="@color/colorHui">

        </View>

        <LinearLayout
            android:id="@+id/relativeLayout26"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_alignParentLeft="true"
            android:layout_marginBottom="5dp"
            android:layout_marginTop="5dp"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/relativeLayout25">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="5dp"
                android:text="@string/queren_mima"
                android:textColor="@color/colorHui"
                android:textSize="13dp" />

            <EditText
                android:id="@+id/editText7"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignParentBottom="true"
                android:layout_toEndOf="@+id/imageView46"
                android:layout_toRightOf="@+id/imageView46"
                android:background="@drawable/no_border"
                android:ems="10"
                android:textCursorDrawable="@drawable/color_cursor_white"
                android:inputType="textPassword"
                android:paddingLeft="10dp"
                android:textColor="@color/white"
                android:textSize="17dp" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_below="@+id/relativeLayout26"
            android:background="@color/colorHui">

        </View>

        <TextView
            android:id="@+id/textView51"
            android:layout_width="200dp"
            android:layout_height="40dp"
            android:layout_below="@+id/relativeLayout26"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="100dp"
            android:background="@mipmap/logo_dengl"
            android:gravity="center"
            android:onClick="doResetPassword"
            android:text="@string/queren"
            android:textColor="@android:color/black"
            android:textSize="16dp" />

    </RelativeLayout>

</RelativeLayout>
