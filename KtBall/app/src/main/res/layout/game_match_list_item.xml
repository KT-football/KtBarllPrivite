<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="90dp"
    android:background="@android:color/black"
    android:layout_marginBottom="4dp">

    <ImageView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/imageView89"
        android:scaleType="fitXY"
        android:layout_marginRight="16dp"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="220dp"
        android:layout_height="40dp"
        android:text="name"
        android:textSize="17dp"
        android:textColor="@android:color/white"
        android:id="@+id/textView115"
        android:layout_alignTop="@+id/imageView89"
        android:layout_toRightOf="@+id/imageView89"
        android:layout_toEndOf="@+id/imageView89" />

    <ImageView
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:id="@+id/imageView90"
        android:src="@drawable/right_icon"
        android:layout_marginRight="16dp"
        android:layout_centerVertical="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="220dp"
        android:layout_height="40dp"
        android:text="place"
        android:textSize="17dp"
        android:textColor="@android:color/white"
        android:id="@+id/textView116"
        android:layout_below="@+id/textView115"
        android:layout_toRightOf="@+id/imageView89"
        android:layout_toEndOf="@+id/imageView89" />

    <RelativeLayout
        android:layout_width="29dp"
        android:layout_height="90dp"
        android:layout_marginRight="16dp"
        android:layout_alignTop="@+id/textView115"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"></RelativeLayout>
</RelativeLayout>