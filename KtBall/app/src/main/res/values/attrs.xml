<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="newProgressBar">
        <attr name="outsideColor" format="color"/>
        <attr name="centreColor" format="color"/>
        <attr name="buttonColor" format="color"/>
        <attr name="progressColorS" format="color"/>
        <attr name="progressColorP" format="color"/>
        <attr name="progressColorF" format="color"/>
        <attr name="buttonWidth" format="dimension"/>
        <attr name="buttonMax" format="integer"/>
        <attr name="lenCentre" format="dimension"/>
        <attr name="lenOutside" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="spiderProgressBar">
        <attr name="spiderProgressBarColor" format="color"/>

    </declare-styleable>

    <!--扩散圆颜色-->
    <attr name="diffuse_color" format="color"/>
    <!--中心圆颜色-->
    <attr name="diffuse_coreColor" format="color"/>
    <!--中心圆图片-->
    <attr name="diffuse_coreImage" format="reference"/>
    <!--中心圆半径-->
    <attr name="diffuse_coreRadius" format="float"/>
    <!--扩散圆宽度,值越小越宽-->
    <attr name="diffuse_width" format="integer"/>
    <!--最大扩散宽度-->
    <attr name="diffuse_maxWidth" format="integer"/>

    <declare-styleable name="DiffuseView">
        <attr name="diffuse_color"/>
        <attr name="diffuse_coreColor"/>
        <attr name="diffuse_coreImage"/>
        <attr name="diffuse_coreRadius"/>
        <attr name="diffuse_width"/>
        <attr name="diffuse_maxWidth"/>
    </declare-styleable>
</resources>